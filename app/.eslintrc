{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "eslint-config-airbnb"
  ],
  "rules": {
    "react/require-default-props": "off",
    "react/forbid-prop-types": "off",
    "arrow-body-style": "off",
    "no-return-await": "off",
    "max-len": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "strict": "off"
  },
  "overrides": [
  ]
}
